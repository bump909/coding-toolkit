{
    "name": "bump909/coding-toolkit",
    "description": "A toolkit containing Laravel macros, helpers, and MySQL patterns.",
    "type": "project",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "laravel/app/",
            "MysqlPatterns\\": "mysql-patterns/"
        }
    },
    "scripts": {
        "test": "phpunit --testdox tests/"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}